alias bashconfig="nano ~/.bashrc"
alias pubkey="cat ~/.ssh/id_dsa.pub"
alias sshconfig="nano ~/.ssh/config"
alias pwgen="openssl rand -base64 16"
alias tinifier="~/scripts/tinifier/tinifier compress -k 'APIKEY'"
alias dssh="ddev ssh"
alias dstart="ddev start"
alias dstop="ddev stop"
alias dpoweroff="ddev poweroff"
alias drestart="ddev restart"
alias hostsconfig="sudo nano /etc/hosts"
