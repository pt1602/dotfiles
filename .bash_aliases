alias bashconfig="nvim ~/.bashrc"
alias pubkey="cat ~/.ssh/id_dsa.pub"
alias sshconfig="nvim ~/.ssh/config"
alias pwgen="openssl rand -base64 16"
alias dssh="ddev ssh"
alias dstart="ddev start"
alias dstop="ddev stop"
alias dpoweroff="ddev poweroff"
alias drestart="ddev restart"
alias dexec="ddev exec"
alias dadminer="ddev adminer"
alias hostsconfig="sudo nvim /etc/hosts"
alias update="sudo apt update -y && sudo apt dist-upgrade -y && sudo apt autoremove -y"
